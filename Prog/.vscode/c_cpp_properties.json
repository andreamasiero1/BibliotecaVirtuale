{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/lib/QtWidgets.framework/Headers",
                "/opt/homebrew/lib/QtGui.framework/Headers", 
                "/opt/homebrew/lib/QtCore.framework/Headers",
                "/opt/homebrew/include",
                "/opt/homebrew/include/QtCore",
                "/opt/homebrew/include/QtGui",
                "/opt/homebrew/include/QtWidgets"
            ],
            "defines": [
                "QT_WIDGETS_LIB",
                "QT_GUI_LIB", 
                "QT_CORE_LIB"
            ],
            "macFrameworkPath": [
                "/opt/homebrew/lib"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}